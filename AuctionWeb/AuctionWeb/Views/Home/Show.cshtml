@{
    ViewBag.Title = "Show";
    Layout = "~/Views/Shared/FrontMaster.cshtml";
}

<div class="positions"><h1>在线预展</h1></div>
<div class="content" id="show">
    @{
        List<AuctionWeb.Models.GoodsInfo> mlist = (List<AuctionWeb.Models.GoodsInfo>)ViewData["List"];
       
        foreach (var item in mlist)
        {
            <dl>
                <dt><a href="/Show/ProInfo/@item.Id"><img src="@item.ImagePath" /></a></dt>
                <dd><a href="/Show/ProInfo/@item.Id">@item.GoodsName</a>
                【@item.Status】
                </dd>
            </dl>
        }
    }
    <div class="clear"></div>
</div><!--content/-->

<p>
    <div class="yahoo2">
        @Html.Raw(ViewData["PageBar"])
    </div>
</p>
